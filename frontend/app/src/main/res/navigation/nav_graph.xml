<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.censobrasilapp.FirstFragment"
        android:label="@string/inicio_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.censobrasilapp.SecondFragment"
        android:label="@string/faces_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FaceTela"
            app:destination="@id/FaceTela" />
    </fragment>
    <fragment
        android:id="@+id/FaceTela"
        android:name="com.censobrasilapp.FaceTela"
        android:label="@string/adc_face_label"
        tools:layout="@layout/face_tela">

        <action
            android:id="@+id/action_FaceTela_to_UnidadeTela"
            app:destination="@id/UnidadeTela" />
    </fragment>

    <fragment
        android:id="@+id/UnidadeTela"
        android:name="com.censobrasilapp.UnidadeTela"
        android:label="@string/adc_unidade_label"
        tools:layout="@layout/unidade_tela">
    </fragment>
</navigation>